    public void SetRegistro(int rnuevo,String rut, String dv,String nombre, int codtipo, String dias_horas, String desde, String hasta, String obs, String id){
        
        ncod = codtipo;
        Desde = desde;
        Hasta = hasta;
        txRut.setText(rut);
        Dvp = dv;
        txNombre.setText(nombre);
        txNdias_horas.setText(dias_horas);
        
        nuevo = rnuevo;
        
        if (nuevo == 1){                //si existe el registro
            
            Id = id;
            
            jLabel1.setText(Id);
            btEditar.setEnabled(true);
       
            btGuardar.setEnabled(false);
            btCancelar.setEnabled(false);
            btImprimir.setEnabled(true);
        
            txRut.setEnabled(false);
            txNombre.setEnabled(false);
            txNdias_horas.setEnabled(false);
            dtDesde.setEnabled(false);
            dtDesde.getEditor().setEditable(false); 
            
            dtHasta.setEnabled(false);
            dtHasta.getEditor().setEditable(false); 
            
            txObs.setEnabled(false);
        
        }else if (nuevo == 0){          //si es un nuevo registro
           
           Id = ""; 
           btEditar.setEnabled(false);
       
           btGuardar.setEnabled(true);
           btCancelar.setEnabled(true);
           btImprimir.setEnabled(false);
        
           txRut.setEnabled(false);
           txNombre.setEnabled(false);
           txNdias_horas.setEnabled(true);
           dtDesde.setEnabled(true);
           dtDesde.getEditor().setEditable(false); 
           
          
           
           
           if (ncod != 3) {
           
              dtHasta.setEnabled(true);
           }
           dtHasta.getEditor().setEditable(false); 
           txObs.setEnabled(true); 
        
        }
        
        
         if (ncod == 1) {
              
              btImprimir.setVisible(true);
              //dtHasta.setEnabled(false);
              dtHasta.getEditor().setEditable(false); 
         }
        
        
        if (ncod == 1 || ncod == 2 || ncod == 4 ){              //Feriado Legal  || Licencia Médica  || Ausentismo
            
           jLabel17.setText(jLabel17.getText()+"dias");
        
        }else if (ncod == 3) {
             
           jLabel17.setText(jLabel17.getText()+"horas");
           dtHasta.setEnabled(false);
        
        }
        
        try{
            
            dtDesde.setDate(dateFormat.parse(Desde));
            dtHasta.setDate(dateFormat.parse(Hasta));
            
        }catch (ParseException ex) {
            
            Logger.getLogger(jdPersonal_Plhx.class.getName()).log(Level.SEVERE, null, ex);
        }
        txObs.setText(obs);
    }
   
