        guardado = false; 
          
        if (nuevo == 0){
       
           dispose(); 
       
        }else if (nuevo == 1){
         
            btEditar.setEnabled(true);
       
            btGuardar.setEnabled(false);
            btCancelar.setEnabled(false);
            btImprimir.setEnabled(true);
        
            txRut.setEnabled(false);
            txNombre.setEnabled(false);
            txNdias_horas.setEnabled(false);
            dtDesde.setEnabled(false);
            dtDesde.getEditor().setEditable(false); 
            dtHasta.setEnabled(false);
            dtHasta.getEditor().setEditable(false); 
            txObs.setEnabled(false); 
       
        }
       
    }//GEN-LAST:event_btCancelarActionPerformed

    private void btGuardarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btGuardarActionPerformed
        
        if (txNombre.getText().equals("")){
           
            fmMain.Mensaje("Debe Ingresar Rut del Trabajador");
            txRut.requestFocus();
            return;
        
        }
        
        if (txNdias_horas.getText().equals("") || txNdias_horas.getText().equals("0")){
           
            fmMain.Mensaje("Debe Ingresar N° de dias/horas");
            return;
       
        }  

        if (dtDesde.getDate() == null){
           
            fmMain.Mensaje("Debe Ingresar fecha Inicial");
            return;
        
        }
        
        if (dtHasta.getDate() == null){
           
            fmMain.Mensaje("Debe Ingresar fecha Final");
            return;
        
        }
        
        btEditar.setEnabled(true);
        btGuardar.setEnabled(false);
        btCancelar.setEnabled(false);
        btImprimir.setVisible(true);
        
        txNombre.setEnabled(false);
        txNdias_horas.setEnabled(false);
        dtDesde.setEnabled(false);
        dtHasta.setEnabled(false);
        txObs.setEnabled(false);
        
        ExeSql Sql = new ExeSql();
        
        if (nuevo == 0){
            
            try{
                 
                if (ncod == 1){
                    
                    Id = "FL"+getDesdeAsString().substring(2);             //FERIADO LEGAL
                
                } 
                
                if (ncod == 2){
                    
                    Id = "LM"+getDesdeAsString().substring(2);             //LICENCIA MEDICA   
                
                }
                
                if (ncod == 3){
                    
                    Id = "HE"+getDesdeAsString().substring(2);             //HORAS EXTRAS   
                
                }
                
                if (ncod == 4 && !chkRetiro.isSelected()){
                    
                    Id = "AU"+getDesdeAsString().substring(2);             //AUSENTISMO 
                
                }else if (ncod == 4 && chkRetiro.isSelected()){            
                
                    Id = "FIN"+getDesdeAsString().substring(2);             //RETIRO (Renuncia o Despido)
                
                }
                
                
                
                 Sql.ExeSql("INSERT INTO personal_plhx(rut,ndias_horas,dfecha,hfecha,tipo,observacion,id) \n" +
                            "VALUES (" + txRut.getText() + "," + txNdias_horas.getText() + ",'"+getDesdeAsString()+"','"+
                            getHastaAsString()+"',"+ncod+",'"+txObs.getText()+"','"+Id+"')");
             
                 guardado = true;    
            
            }catch (SQLException ex) {
                    
                    Sql.Rollback();
                    
                    Logger.getLogger(jdPersonal_Plhx.class.getName()).log(Level.SEVERE, null, ex);
                    guardado = false;
                    
            }
                
        }else if (nuevo == 1){
        
            try{
                Sql.ExeSql("UPDATE personal_plhx SET \n"+
                           "ndias_horas=" + txNdias_horas.getText() + ", \n" +
                           "dfecha='" + getDesdeAsString() + "'," +
                           "hfecha='" + getHastaAsString() + "'," +
                           "observacion='" + txObs.getText() + "'\n" +
                           "WHERE rut=" + txRut.getText() +  "\n" +
                           "AND dfecha= '" + getDesdeAsString() + "'\n" +
                           "AND tipo=" + ncod);
                
                Sql.Commit();
                guardado = true;   
                
            }catch (SQLException ex) {
                
                Sql.Rollback();
                Logger.getLogger(jdPersonal_Plhx.class.getName()).log(Level.SEVERE, null, ex);
                guardado = false;
            }
        
        }
        
        if (guardado){
               
            fmMain.Mensaje("Registro Guardado con éxito");
            dispose(); 
                
        }        
        
    }//GEN-LAST:event_btGuardarActionPerformed

    private void btEditarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btEditarActionPerformed
       
       intNivelUsuario = fmMain.trae_nivel(fmMain.GetUsuario());
     
        if (( intNivelUsuario < 85)){    
           
           fmMain.Mensaje("Usuario: " + fmMain.GetUsuario() + " no está autorizado") ;
