    private void dtDesdeActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_dtDesdeActionPerformed
        
        
        if(ncod == 1){
        
            int ndias = Integer.parseInt(txNdias_horas.getText());
        
            dias.setTime(dtDesde.getDate());
            dias.add(Calendar.DAY_OF_MONTH,ndias-1);
            dias.getTime();
            dtHasta.setDate(dias.getTime());
        
         }
        
        
        if(ncod == 3){
           
          dtHasta.setDate(dtDesde.getDate());
        
        }
    }//GEN-LAST:event_dtDesdeActionPerformed

