    private void txNdias_horasKeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_txNdias_horasKeyTyped
        char car = evt.getKeyChar();
        
        if (car=='.'){   
           
           char x;
           int conta=0;
           
           for (int y=0; y < txNdias_horas.getText().length(); y++ ){
                        
                x = txNdias_horas.getText().charAt(y);
                 
                if (x=='.'){
                   conta++;
                }
                
                if (conta==1){
                    evt.consume(); 
                }
           }
        }
            
        if(((car<'0') || (car>'9')) && ((car<'.') || (car>'.'))){
           evt.consume(); 
        }        
    }//GEN-LAST:event_txNdias_horasKeyTyped

    private void btImprimirActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btImprimirActionPerformed
        
         try {
            
             
           jdGetFeriados feriados = new jdGetFeriados(null, true);
           feriados.setLocationRelativeTo(null);
           feriados.setTitle("DIAS FERIADOS");
           feriados.setVisible(true);
           
           int forma = Integer.parseInt(txNdias_horas.getText());
           
                  
//            String dato,nombre = "";
            Map parametro = new HashMap();
            
            parametro.put("LOGO",getClass().getResourceAsStream("/Reportes/Logo.jpg"));                     //Ruta Logo Reporte 
            
            parametro.put("NOMBRE",txNombre.getText());
            parametro.put("RUT",formatearRut(txRut.getText()+Dvp));
            
            if (forma < 15){
            
                 parametro.put("FORMA","PARCIAL");
            
            }else if (forma == 15){
            
                parametro.put("FORMA","COMPLETA");
            
            }
            
            
            parametro.put("PERIODO",feriados.GetPeriodo());
           
            parametro.put("FECHAS","Del "+dateFormat.format(dtDesde.getDate())+"  al  "+dateFormat.format(dtHasta.getDate()));
            parametro.put("DIAS",txNdias_horas.getText());
            parametro.put("FERIADOS",feriados.GetDiasFeriados());
            
            
           
            
            JasperReport reporte = (JasperReport) JRLoader.loadObject(getClass().getResource("/Reportes/rpFeriados.jasper")); //Ruta del Reporte
            JasperPrint jprint = JasperFillManager.fillReport(reporte, parametro,new JREmptyDataSource());
           
            JasperViewer view = new JasperViewer(jprint,false);
            view.setDefaultCloseOperation(DISPOSE_ON_CLOSE);
            view.setVisible(true);
            
            
        }catch (JRException ex) {
        
            ex.printStackTrace(); 
            System.out.println ("informes r" + ex.toString ());
            Logger.getLogger(jdPersonal_Plhx.class.getName()).log(Level.SEVERE, null, ex);
        
        }
        
        
        
    }//GEN-LAST:event_btImprimirActionPerformed

