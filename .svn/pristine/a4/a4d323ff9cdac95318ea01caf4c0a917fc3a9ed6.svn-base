    private void btIrActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btIrActionPerformed
        
        if (!txRut.getText().isEmpty()) {   //Codigo agregado por R. Lopez    // Si Rut no est谩 vac铆o
            
            txDv.setText(generaDigito(txRut.getText().trim()+"-X"));
            
            if(!fmMain.validarRut(txRut.getText().trim() + "-" + txDv.getText().trim())){ //Se activ贸 rutina 
                 
                fmMain.Mensaje("El Rut es incorrecto");                                   //para validar rut
                txRut.requestFocus();
                 
             }else{
                
                CargaPersonal(txRut.getText().trim());                          
             }
        
        }else{
                        
            jdAgregaDato Dato = new jdAgregaDato(null, true);           
            Dato.setLocationRelativeTo(null);
            Dato.setTitle("Trabajador");
            Dato.CargaDatos("SELECT rut AS codigo,nombre FROM personal WHERE rut>1000 AND activo = true \n" +
                            "AND empresa ="+1+" ORDER BY nombre ");
            Dato.setVisible(true);
            
            if (!Dato.GetCodigo().isEmpty()) {
                CargaPersonal(Dato.GetCodigo());
                btIr.setEnabled(false);
            }else{
                SetEstado(1);
                btIr.setEnabled(false);
            }
            
        }
    }//GEN-LAST:event_btIrActionPerformed

    private void btAgregaHActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btAgregaHActionPerformed
        
        String Codigos = "";
        String Query="";
                
        if(GrillaHaberes.getRowCount()==0){
        
            Query = "SELECT codigo,nombre FROM par_general WHERE tipo='REM_HABERES' ORDER BY nombre ASC";
        
        }else{
            
            for(int i=0; i< GrillaHaberes.getRowCount();i++){
                
                if (!txIdcontrato.getText().equals("62") || txRut.getText().equals("15986675")){
                
                    Codigos = Codigos + "38,";
                }
                
                Codigos = Codigos + "4,9,10,36,39,";   
                                
                Codigos = Codigos + GrillaHaberes.getValueAt(i,1).toString().trim() + ",";   
            }
            
            if(Codigos.length()>0)
               
               Codigos = Codigos.substring(0, Codigos.length()-1);
            
            Query = "SELECT codigo,nombre FROM par_general WHERE tipo='REM_HABERES' AND codigo NOT IN ("+ Codigos +") ORDER BY nombre ASC";
        } 
        
        jdAgregaDato Dato = new jdAgregaDato(null, true);
        Dato.setLocationRelativeTo(null);
        Dato.setTitle("Haberes");
        Dato.CargaDatos(Query);
        Dato.setVisible(true);
        
        try{
             
            jdBuscaIndicadores bonos = new jdBuscaIndicadores();          
        
            if (!Dato.GetCodigo().isEmpty()) {                                              //Se agrega valor monto al momento de agregar un HABER
           
                DefaultTableModel Model = (DefaultTableModel)GrillaHaberes.getModel();
 
                if (Dato.GetCodigo().equals("0") || Dato.GetCodigo().equals("2") || Dato.GetCodigo().equals("3")) { 
                
                   jdGetMonto Monto = new jdGetMonto(null, true); 
                   Monto.setLocationRelativeTo(null);
                   Monto.setTitle("Haberes");
                   Monto.setVisible(true);
                   dmonto = Double.parseDouble(Monto.GetMonto()); 
               
                   String vmonto = "$ "+decimal.format(dmonto);
                   Model.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),vmonto});  
               
                }else if (Dato.GetCodigo().equals("38")) {                      //BONO FACTURACION
              
                   cbono = bonos.Asigna_cod_bono(txRut.getText(),Dato.GetCodigo()); 
                
                   Model.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),0,cbono});  
               
                }else if (Dato.GetCodigo().equals("28")) {                       //COMISION POR VENTAS
              
                   cbono = bonos.Asigna_cod_bono(txRut.getText(),Dato.GetCodigo()); 
                
                   Model.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),0,cbono});  
               
                }else if (Dato.GetCodigo().equals("37")) {                      //BONO CUMPLIMIENTO PRESUPUESTO
              
                   cbono = bonos.Asigna_cod_bono(txRut.getText(),Dato.GetCodigo()); 
                
                   Model.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),0,cbono});  
               
                }else if (Dato.GetCodigo().equals("11")) {                      //COMISION GESTION COBRANZA
              
                   cbono = bonos.Asigna_cod_bono(txRut.getText(),Dato.GetCodigo()); 
                
                   Model.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),0,cbono});  
               
                }else{  
                   
                    Model.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),0,0});
                }
            }
        
        
        } catch (IOException ex) {
            Logger.getLogger(pfPersonal.class.getName()).log(Level.SEVERE, null, ex);
        }
