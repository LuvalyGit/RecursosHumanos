                             "WHERE rut=" + RutGeneral);
                       
                }else if (nuevo == 0){                            //C贸digo agregado Roberto Lopez 
                            
                            RutGeneral=txRut.getText().trim();     //Se agreg贸 sentencia sql para guardar un nuevo registro
                            Sql.ExeSql("INSERT INTO personal(rut,nombre,apellidopaterno,apellidomaterno,sexo,direccion,poblacion,ciudad,\n" +
