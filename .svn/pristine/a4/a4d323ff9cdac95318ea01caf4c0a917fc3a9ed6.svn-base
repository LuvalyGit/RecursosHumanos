                try{    txDireccion.setText(Rs.getString("direccion").trim());      }catch(Exception e){    txDireccion.setText("");}
                try{    txPoblacion.setText(Rs.getString("poblacion").trim());      }catch(Exception e){    txPoblacion.setText("");}
                try{    txCiudad.setText(Rs.getString("ciudad").trim());            }catch(Exception e){    txCiudad.setText("");}
                try{    txCelular.setText(Rs.getString("celular").trim());          }catch(Exception e){    txCelular.setText("");}
                try{    txEmail.setText(Rs.getString("mail").trim());               }catch(Exception e){    txEmail.setText("");}
                try{    txDepaId.setText(Rs.getString("iddepto").trim());           }catch(Exception e){    txDepaId.setText("");}
                try{    txIdcargo.setText(Rs.getString("iddepto").trim());          }catch(Exception e){    txIdcargo.setText("");}
                try{    txNcuenta.setText(Rs.getString("ctabank").trim());          }catch(Exception e){    txNcuenta.setText("");}
                try{    txCtaId.setText(Rs.getString("codigo").trim());             }catch(Exception e){    txCtaId.setText("");}
                //try{    txTcuenta.setText(Rs.getString("cta").trim());              }catch(Exception e){    txTcuenta.setText("");}
                try{    txTcuenta.setText(tipcuenta);                               }catch(Exception e){    txTcuenta.setText("");}
                try{    txFecNac.setText(Rs.getString("fechan").trim());            }catch(Exception e){    txFecNac.setText("");}
                try{    dtIngreso.setDate(Rs.getDate("fecing"));                    }catch(Exception e){    dtIngreso.setDate(null);}  
                try{    txNcargas.setText(Rs.getString("ncargas"));                 }catch(Exception e){    txNcargas.setText("");}  
                try{    txFeriado.setText(Rs.getString("dferiado"));                }catch(Exception e){    txFeriado.setText("");}  
                
                
                CargaAbonosDescuentos(Rut);
                Habilitar(false);
                btEditar.setEnabled(true);  //Se habilita el bot贸n "editar" al cargar 
                
            }
            
             if(Sql3.GetRowCount()>0 ){
                 
                Rs3.next();
                try{    txIdcontrato.setText(Rs3.getString("codigo").trim());       }catch(Exception e){    txIdcontrato.setText("");}
                try{    txTcontrato.setText(Rs3.getString("cnt").trim());           }catch(Exception e){    txTcontrato.setText("");}    
            }    
            
            
            if(Sql2.GetRowCount()>0 ){
                
                Rs2.next();
                try{    txBancId.setText(Rs2.getString("codigo").trim());           }catch(Exception e){    txBancId.setText("");}
                try{    txBanco.setText(Rs2.getString("bnc").trim());               }catch(Exception e){    txBanco.setText("");}    
