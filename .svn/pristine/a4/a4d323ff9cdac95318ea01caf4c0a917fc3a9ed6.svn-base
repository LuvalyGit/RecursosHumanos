    private void btAgregaDActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btAgregaDActionPerformed
        
        String Codigos = ""; 
        String Query = "";
        double porcentaje = 0;
        
        if(GrillaDescuentos.getRowCount()==0)
            
            Query = "SELECT codigo,nombre FROM par_general WHERE tipo='REM_DESCUENTOS' ORDER BY nombre ASC";
        
        else{
            
            for(int i=0; i< GrillaDescuentos.getRowCount();i++){
                
                if (GrillaDescuentos.getValueAt(i,1).toString().equals("5")){
                
                    Codigos = Codigos + "5,17,18,24,34,42,43,44,";
                
                }if (GrillaDescuentos.getValueAt(i,0).toString().contains("AFP")){
                 
                    Codigos = Codigos + "12,15,16,21,22,23,201,";
                
                }if (GrillaDescuentos.getValueAt(i,0).toString().contains("ISAPRE")){
                
                    Codigos = Codigos + "5,17,24,34,42,43,44,"; 
                
                }if (GrillaDescuentos.getValueAt(i,1).toString().equals("25")){
                
                    Codigos = Codigos + "33,";
                
                }if (GrillaDescuentos.getValueAt(i,1).toString().equals("33")){
                
                    Codigos = Codigos + "25,";
                
                }if (!txIdcontrato.getText().equals("62")){
                
                    Codigos = Codigos + "14,";
                }
                 
                Codigos = Codigos + "19,27,31,40,78,"+GrillaDescuentos.getValueAt(i,1).toString().trim() + ",";
            }
            
            if(Codigos.length()>0){
                
               Codigos = Codigos.substring(0, Codigos.length()-1);
               Query = "SELECT codigo,nombre FROM par_general WHERE tipo='REM_DESCUENTOS' \n"+ 
                       "AND codigo NOT IN ("+ Codigos +") ORDER BY nombre ASC";
            }
        } 
        
        jdAgregaDato Dato = new jdAgregaDato(null, true);
        Dato.setLocationRelativeTo(null);
        Dato.setTitle("Descuentos");
        Dato.CargaDatos(Query);
        Dato.setVisible(true);
        
        if (!Dato.GetCodigo().isEmpty()) {
            
            if (Dato.GetNombre().contains("AFP")) {           
                try {    
                    jdBuscaIndicadores afp = new jdBuscaIndicadores();
                    if (Dato.GetCodigo().equals("12")){
                        porcentaje = afp.GetPlanVital();
                    }
                    if (Dato.GetCodigo().equals("15")){
                        porcentaje = afp.GetCapital();
                    }
                    if (Dato.GetCodigo().equals("16")){
                        porcentaje = afp.GetHabitat();
                    }
                    if (Dato.GetCodigo().equals("21")){
                       porcentaje = afp.GetProVida();
                    }
                    if (Dato.GetCodigo().equals("22")){
                       porcentaje = afp.GetModelo();
                    }
                    if (Dato.GetCodigo().equals("23")){
                       porcentaje = afp.GetCuprum();
                    } 
                    if (Dato.GetCodigo().equals("201")){
                       porcentaje = afp.GetUno();
                    }   
                
                } catch (IOException ex) {
                  Logger.getLogger(pfPersonal.class.getName()).log(Level.SEVERE, null, ex);
                }
            }else if (Dato.GetCodigo().equals("5")){
            
                porcentaje = 0.07;
            
            }else if (Dato.GetCodigo().equals("13")){
            
                porcentaje = 0.1;
            
            }else if (Dato.GetCodigo().equals("14")){
            
                porcentaje = 0.006;

            }else if (Dato.GetCodigo().equals("17") || Dato.GetCodigo().equals("24") || Dato.GetCodigo().equals("34") ||
                      Dato.GetCodigo().equals("42") || Dato.GetCodigo().equals("43") || Dato.GetCodigo().equals("44")){
      
                    porcentaje = 0.07;       
                
            }
            
            
            if (!Dato.GetCodigo().isEmpty()) {                                              
                
                DefaultTableModel Model2 = (DefaultTableModel)GrillaDescuentos.getModel();
                
                if (Dato.GetCodigo().equals("18")) {                                //ADICIONAL ISAPRE
                    
                    try {                
                        
                        jdGetUf ufIsapre = new jdGetUf(null, true);
                        ufIsapre.setLocationRelativeTo(null);
                        ufIsapre.setTitle("Descuentos");
                        ufIsapre.setVisible(true);
                        dmonto = ufIsapre.GetUf();
                        
                        Model2.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),"",dmonto});
                            
                        
                    }catch(NumberFormatException e ){
                    
                        fmMain.Mensaje("Formato no v谩lido"); 
                    }
               
                }else if (Dato.GetCodigo().equals("25")){               //APV A
            
                      porcentaje = 1;
                
                      jdGetMonto MontoApv = new jdGetMonto(null, true);
                      MontoApv.setLocationRelativeTo(null);
                      MontoApv.setTitle("APV A");
                      MontoApv.setVisible(true);
                      dmonto = Double.parseDouble(MontoApv.GetMonto()); 
                      String vmonto = "$ "+decimal.format(dmonto);
                      Model2.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),vmonto,porcentaje});
               
                }else if (Dato.GetCodigo().equals("33")){               //APV B
            
                      porcentaje = 2;
                
                      jdGetMonto MontoApv = new jdGetMonto(null, true);
                      MontoApv.setLocationRelativeTo(null);
                      MontoApv.setTitle("APV B");
                      MontoApv.setVisible(true);
                      dmonto = Double.parseDouble(MontoApv.GetMonto()); 
                      String vmonto = "$ "+decimal.format(dmonto);
                      Model2.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),vmonto,porcentaje});
               
                }else if (Dato.GetCodigo().equals("74")){               //CUENTA AHORRO PREVISIONAL
            
                      porcentaje = 0;
                
                      jdGetMonto MontoApv = new jdGetMonto(null, true);
                      MontoApv.setLocationRelativeTo(null);
                      MontoApv.setTitle("CUENTA AHORRO PREVISIONAL");
                      MontoApv.setVisible(true);
                      dmonto = Double.parseDouble(MontoApv.GetMonto()); 
                      String vmonto = "$ "+decimal.format(dmonto);
                      Model2.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),vmonto,porcentaje});
               
                }else{
                    
                    Model2.addRow(new Object[]{Dato.GetNombre(),Dato.GetCodigo(),"",porcentaje});
                }
            }
           
        }
    }//GEN-LAST:event_btAgregaDActionPerformed

