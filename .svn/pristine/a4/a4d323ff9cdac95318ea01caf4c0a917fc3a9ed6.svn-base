    private void dtIngresoKeyTyped(java.awt.event.KeyEvent evt) {//GEN-FIRST:event_dtIngresoKeyTyped
        
        char car = evt.getKeyChar();
        
        if(((car<'0') || (car>'9')) && ((car<'.') || (car>'.'))){
            
