                                     Math.round(result3)+","+0+","+valordia+","+0+","+1+")");
                            
                       }else{
                        
                          String codigo = GrillaHaberes.getValueAt(i, 1).toString(); 
                          
                          if(codigo.equals("38") || codigo.equals("28") || codigo.equals("37") || codigo.equals("11") ){
                            
                              Sql.ExeSql("INSERT INTO personalhd (rut,tipo,codigo,valor,porcentaje,cod_bono,empresa) VALUES (" +
                                         RutGeneral +  ",'REM_HABERES'," + GrillaHaberes.getValueAt(i, 1).toString().trim()+ ",\n"+
                                         Math.round(result3)+","+0+","+GrillaHaberes.getValueAt(i, 3).toString().trim()+","+1+")"); 
                              
                          }else{  
                           
                              Sql.ExeSql("INSERT INTO personalhd (rut,tipo,codigo,valor,porcentaje,cod_bono,empresa) VALUES (" +
                                         RutGeneral +  ",'REM_HABERES'," + GrillaHaberes.getValueAt(i, 1).toString().trim()+ ",\n"+
                                         Math.round(result3)+","+0+","+0+","+1+")");
                          }
                       }
                    }
                }   
               
                for(int i=0;i<GrillaDescuentos.getRowCount();i++){
                   
                     double result1 = Double.parseDouble(GrillaDescuentos.getValueAt(i, 3).toString().trim());               //Porcentaje
                   
                    if (GrillaDescuentos.getValueAt(i, 2).toString().equals("")){          //Si el Monto est谩 en blanco
                                           
                        Sql.ExeSql("INSERT INTO personalhd (rut,tipo,codigo,valor,porcentaje,cod_bono,empresa) VALUES (" +
                                    RutGeneral +  ",'REM_DESCUENTOS'," + GrillaDescuentos.getValueAt(i, 1).toString().trim()+",\n"+
                                    0+","+Precision.round(result1,4) +","+0+","+1+")");
                    }else{
                    
                        String result = GrillaDescuentos.getValueAt(i, 2).toString().trim().replaceAll("\\$","");     //Monto 
                        String result2 = result.trim().replaceAll("\\.","");
                        double result3 = Double.parseDouble(result2.trim());
                   
                        Sql.ExeSql("INSERT INTO personalhd (rut,tipo,codigo,valor,porcentaje,cod_bono,empresa) VALUES (" +
                                    RutGeneral +  ",'REM_DESCUENTOS'," + GrillaDescuentos.getValueAt(i, 1).toString().trim()+",\n"+
                                    Math.round(result3)+","+result1+","+0+","+1+")");
                    }
               }
               
               Sql.Commit();
               Sql.Close();
               
               if (rutv){                       //si rut es correcto
                  
                  SetEstado(1);                 //C贸digo agregado por Roberto Lopez
                  
                  btAgregaD.setEnabled(false);     //se establecen m茅todos para
                  btAgregaH.setEnabled(false);     //botones grilla de departamentos
                  btEliminaD.setEnabled(false);    //cuando se ha agregado un registro nuevo
                  btEliminaH.setEnabled(false);
                  btEditaH.setEnabled(false);
                  btDepartamento.setEnabled(false);
                  btCargo.setEnabled(false);
                  btCuenta.setEnabled(false);
                  btBanco.setEnabled(false);     
                  btIr.setEnabled(false);          //Fin c贸digo agregado
                  btEditaD.setVisible(false);
                  
                  cbSexo.setEnabled(false);
                  
                  CargaPersonal(RutGeneral);
               }
          }   
        } catch ( SQLException | NumberFormatException e) {
             fmMain.Mensaje(e.getMessage());
        } 
