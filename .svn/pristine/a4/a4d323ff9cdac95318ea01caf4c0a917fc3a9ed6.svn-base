                    }else{
                        
                       String result = GrillaHaberes.getValueAt(i, 2).toString().trim().replaceAll("\\$","");
                       String result2 = result.trim().replaceAll("\\.","");
                       double result3 = Double.parseDouble(result2.trim());
                      
                       if (GrillaHaberes.getValueAt(i, 1).toString().equals("0") || GrillaHaberes.getValueAt(i, 1).toString().equals("2") ||
                           GrillaHaberes.getValueAt(i, 1).toString().equals("3") ){
                          
                          valordia = result3 / 30;  
                           
                          Sql.ExeSql("INSERT INTO personalhd (rut,tipo,codigo,valor,porcentaje,vdtrab,cod_bono,empresa) VALUES (" +
