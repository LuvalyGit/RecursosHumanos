    private void CargaAbonosDescuentos(String Rut){
        
        DefaultTableModel ModeloH = (DefaultTableModel) GrillaHaberes.getModel();
        DefaultTableModel ModeloD = (DefaultTableModel) GrillaDescuentos.getModel();
        
        fmMain.LimpiaGrilla(ModeloH);
        fmMain.LimpiaGrilla(ModeloD);
        
        ExeSql Sql = new ExeSql();
        String vmonto = "";
        ResultSet Rs;
        
        try {
        
            Rs = Sql.Select("SELECT pg.tipo,pg.codigo,pg.nombre,hd.valor,hd.porcentaje,hd.cod_bono\n" +
                            "FROM personalhd hd \n" +
                            "LEFT JOIN par_general pg on pg.tipo=hd.tipo and pg.codigo=hd.codigo\n" +
                            "WHERE hd.rut="+ Rut +"\n" +
                            "ORDER BY pg.tipo");
            
            while(Rs.next()){

                if(Rs.getString("tipo").trim().equals("REM_HABERES")){
                
                    if (Rs.getString("valor").equals("0")){
                        vmonto = "0";
                    }else{
                       dmonto = Double.parseDouble(Rs.getString("valor")); 
                       vmonto = "$ "+decimal.format(dmonto);
                    }   
                    ModeloH.addRow(new Object[]{Rs.getString("nombre").trim(),Rs.getInt("codigo"),vmonto,Rs.getInt("cod_bono")});
                
                }else if(Rs.getString("tipo").trim().equals("REM_DESCUENTOS")){
                    
                    if (Rs.getString("valor").equals("0")){
                        vmonto = "";
                    }else{
                       dmonto = Double.parseDouble(Rs.getString("valor")); 
                       vmonto = "$ "+decimal.format(dmonto);
                    }   
                    ModeloD.addRow(new Object[]{Rs.getString("nombre").trim(),Rs.getInt("codigo"),vmonto,Rs.getDouble("porcentaje")});
                }        
            }
            
        }catch (SQLException | NumberFormatException e){
            fmMain.Mensaje(e.getMessage());
        }finally{
            Sql.Close();
        }
    };
    
    
    private void CargaPersonal(String Rut){
        
        ExeSql Sql = new ExeSql();
        ExeSql Sql2 = new ExeSql();
        ExeSql Sql3 = new ExeSql();
        ExeSql Sql4 = new ExeSql();
        ResultSet Rs,Rs2,Rs3,Rs4 = null;
        
        try {
          
            Rs = Sql.Select("SELECT pc.nombre AS depto,pc.id,pg.codigo, pg.nombre AS cta,p.*, TO_CHAR ( p.fecnac, 'dd/mm/yyyy') AS fechan\n" +
                            "FROM personal p\n" +
                            "LEFT JOIN pccostos pc ON p.iddepto=pc.id\n" +
                            "LEFT JOIN par_general pg ON p.idctabank=pg.codigo\n" +
                            "WHERE p.rut=" + Rut);
           
            Rs2 = Sql2.Select("SELECT pg.codigo, pg.nombre AS bnc,p.*\n" +
                              "FROM personal p\n" +
                              "LEFT JOIN par_general pg ON p.idbank=pg.codigo\n" +
                              "WHERE p.rut=" + Rut + " AND pg.tipo IN ('BANCOS')");
            
            Rs3 = Sql3.Select("SELECT pg.codigo, pg.nombre AS cnt,p.*\n" +
                              "FROM personal p\n" +
                              "LEFT JOIN par_general pg ON p.idcontrato=pg.codigo\n" +
                              "WHERE p.rut=" + Rut);
            
            
            Rs4 = Sql4.Select("SELECT pg.codigo, pg.nombre AS cargo,p.*\n" +
                              "FROM personal p\n" +
                              "LEFT JOIN par_general pg ON p.idcargo=pg.codigo\n" +
                              "WHERE p.rut=" + Rut);

            
            if(Sql.GetRowCount()>0 ){
                
                Rs.next();
                
                String tipcuenta = "";
                
                txRut.setText(Rs.getString("Rut").trim());
                txDv.setText(Rs.getString("dvp").trim());  //Se carga el DV de la tabla "personal"
                
                if (Rs.getString("codigo").trim().equals("0") || Rs.getString("codigo").trim().equals(null) ){
                
