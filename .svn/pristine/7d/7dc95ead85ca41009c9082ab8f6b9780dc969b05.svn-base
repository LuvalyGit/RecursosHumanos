    public jdValores(java.awt.Frame parent, boolean modal) throws IOException {
        
        super(parent, modal);
        initComponents();
        
        ExeSql Sql = new ExeSql();
        ResultSet Rs = null;
         
        try {
            Rs = Sql.Select("SELECT * FROM indicadores WHERE codigo NOT IN ('DOL-1','UF-1')");
            
            if(Sql.GetRowCount()>0){
                
                pos = 1;
                
                while (Rs.next()){
                    
                    codigo = Rs.getString("codigo");
                    nombre = Rs.getString("nombre");
                    valor = Precision.round((Rs.getDouble("valor")*100),4);
                    
                    if (codigo.equals("AFC-1") && existe1 == 0){
                    
                        txAfc1.setText(""+valor);
                        existe1 = 1;
                    
                    }else if (!codigo.equals("AFC-1") && existe1 == 0){
                    
                        txAfc1.setText("0");
                       
                    }
                    
                    System.out.println("valor : "+txAfc1.getText());
                    
                    
                    if (codigo.equals("AFC-2") && existe2 == 0){
                    
                        txAfc2.setText(""+valor);
                        existe2 = 1;
                    
                    }else if (!codigo.equals("AFC-2") && existe2 == 0){
                    
                        txAfc2.setText("0");
                      
                    }
                    
                    if (codigo.equals("AFC-3") && existe3 == 0){
                    
                        txAfc3.setText(""+valor);
                        existe3 = 1;
                    
                    }else if (!codigo.equals("AFC-3") && existe3 == 0){
                    
                         txAfc3.setText("0");
                    
                    }
                 
                         
                    if (codigo.equals("MUTUAL-1") && existe4 == 0){
                    
                        txMutual.setText(""+valor);
                        existe4 = 1;
                    
                    }else if (!codigo.equals("MUTUAL-1") && existe4 == 0){
                    
                        txMutual.setText("0");
                    
                    }
        
                    
                    if (codigo.equals("SIS-1") && existe5 == 0){
                    
                        txSis.setText(""+valor);
                        existe5 = 1;
                    
                    }else if (!codigo.equals("SIS-1") && existe5 == 0){
                    
                        txSis.setText("0");
                    
                    }          
                
                }
            
            }else{
            
                txAfc1.setText("0");
                txAfc2.setText("0");
                txAfc3.setText("0");
                txMutual.setText("0");
                txSis.setText("0");
                
                existe1 = 0;
                existe2 = 0;
                existe3 = 0;
                existe4 = 0;
                existe5 = 0;
                
            }
            
            
        } catch (SQLException ex) {
            Logger.getLogger(jdValores.class.getName()).log(Level.SEVERE, null, ex);
        }
         
        btCargar.doClick();
    }

