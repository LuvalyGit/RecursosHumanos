    private void btGuardarActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btGuardarActionPerformed
       
        ExeSql Sql = new ExeSql();
         
        if (txAfc1.getText().equals("") || txAfc1.getText().equals("0") ){
        
           fmMain.Mensaje("Debe Ingresar un valor!!");
           txAfc1.requestFocus();
           return;
                
        }
        if (txAfc2.getText().equals("") || txAfc2.getText().equals("0")){
        
           fmMain.Mensaje("Debe Ingresar un valor!!");
           txAfc2.requestFocus();
           return;
                
        }
        if (txAfc3.getText().equals("") || txAfc3.getText().equals("0")){
        
           fmMain.Mensaje("Debe Ingresar un valor!!");
           txAfc3.requestFocus();
           return;
            
                
        }
        
        if (txMutual.getText().equals("") || txMutual.getText().equals("0")){
        
           fmMain.Mensaje("Debe Ingresar un valor!!");
           txMutual.requestFocus();
           return;
                
        }
        
         if (txSis.getText().equals("") || txSis.getText().equals("0")){
        
           fmMain.Mensaje("Debe Ingresar un valor!!");
           txSis.requestFocus();
           return;
                
        }
        
        
        try {
           
            valor = 0;
            for(int i = 1;i <= 5;i++){
            
                
                
                if (i == 1){
                
                    
                   nombre = "Seguro Cesantía (Empresa) Contrato Fijo";
                   valor = (Double.parseDouble(txAfc1.getText().toString())/100);
                   
                   if (existe1 == 0){
                   
                        Sql.ExeSql("INSERT INTO indicadores (codigo, nombre, valor, fecha) VALUES( \n " +
                                   "'AFC-1' ,\n" +
                                   "'"+nombre+"',\n" +
                                   +valor+",\n" + 
                                   "'"+dateFormat.format(hoy)+"')");
                        Sql.Commit();
                   
                   }else if (existe1 == 1){
                   
                        Sql.ExeSql("UPDATE indicadores SET valor = " +valor+", \n" + 
                                   "fecha = '"+dateFormat.format(hoy)+"' \n"+
                                   "WHERE codigo = 'AFC-1'");
                        Sql.Commit();
                   
                   
                   
                   }
                
                
                }else if (i == 2){
                
                   nombre = "Seguro Cesantía (Empresa) Contrato Indefinido";
                   valor = (Double.parseDouble(txAfc2.getText().toString())/100);
                   
                   
                   if (existe2 == 0){
                   
                        Sql.ExeSql("INSERT INTO indicadores (codigo, nombre, valor, fecha) VALUES( \n " +
                                   "'AFC-2' ,\n" +
                                   "'"+nombre+"',\n" +
                                   +valor+",\n" + 
                                   "'"+dateFormat.format(hoy)+"')");
                        Sql.Commit();
                   
                   }else if (existe2 == 1){
                   
                        Sql.ExeSql("UPDATE indicadores SET valor = " +valor+", \n" + 
                                   "fecha = '"+dateFormat.format(hoy)+"' \n"+
                                   "WHERE codigo = 'AFC-2'");
                        Sql.Commit();
                   
                   
                   
                   }
                   
