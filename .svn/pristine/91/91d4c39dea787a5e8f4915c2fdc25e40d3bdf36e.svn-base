    public static void ErrorUsuarioLog(String Usuario,String Error){
        
        ExeSql Sql = new ExeSql();
    
        try{
            Sql.ExeSql("INSERT INTO usuarios_eventos (usuario,tipo)VALUES ('" + Usuario + "','" + Error + "')");
            Sql.Commit();
            
        }catch (Exception e){
            
            Sql.Rollback();
            
        }finally{
            Sql.Close();
        }
    }

